apply Service "SWAN:PORT_app_swan" {
        import "generic-service"
        display_name ="SWAN:PORT_app_swan"
        check_command ="tcp"
        vars.tcp_ssl = false
        vars.tcp_timeout = 60
        vars.tcp_port = 8080
        assign where match("*swan*", host.name)
}

