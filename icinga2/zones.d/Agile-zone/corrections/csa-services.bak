apply Service "CSA:Port_HTTP" {
        import "generic-service"
        display_name ="CSA:Port_HTTP"
        check_command ="http"
       vars.http_ssl = false
        vars.http_timeout = 60
        vars.http_port = 8080
        assign where match("*csa*", host.name)
}



apply Service "CSADB:ORACLE:PORT_1521" {
        import "generic-service"
        display_name ="CSADB:ORACLE:PORT_1521"
        check_command ="tcp"
        vars.nrpe_no_ssl = true
        vars.nrpe_timeout = 60
        vars.nrpe_arguments = ["1521"]
        assign where match("*csadb*", host.name)
}

